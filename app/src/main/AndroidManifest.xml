<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jack.android.tutorial">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalculatorActivity"
            android:label="@string/title_activity_calculator" />
        <activity
            android:name=".FlowLayoutActivity"
            android:label="@string/title_activity_flow_layout" />
        <activity
            android:name=".TopBarActivity"
            android:label="@string/title_activity_top_bar" />
        <activity
            android:name=".ArcMenuActivity"
            android:label="@string/title_activity_arc_menu" />
        <activity
            android:name=".ScratchCardActivity"
            android:label="@string/title_activity_scratch_card" />
    </application>

</manifest>
